{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Proyecto_login_mta-Login",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/proyecto-login-mta-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/logaligroupLogin/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Login",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Proyecto_login_mta/Login\"}"
      },
      "envFile": "${workspaceFolder}/Login/.env5",
      "preLaunchTask": "Build Proyecto_login_mta"
    }
  ]
}